{
  "name": "react-flask-app",
  "version": "0.1.0",
  "private": true,
  "productName": "Electron Create React App Example",
  "main": "electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "ps-tree": "^1.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "electron-prebuilt": "electron .",
    "electron-start":"npm run build && electron .",
    "pyinstaller_flask": "pyinstaller -F ./python/app.py --workpath ./python_build --distpath ./python_dist --specpath ./python_spec",
    "clean":"rm -rf python_build python_dist python_spec build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^9.1.1"
  }
}
